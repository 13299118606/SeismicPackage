#! /bin/csh -f
#-------------------------------------------------------------------------------
#                           clean_autogen_files
#-------------------------------------------------------------------------------

foreach file (*)
   grep 'DO NOT EDIT THIS FILE - it is machine generated' $file > /dev/null || continue
   echo "\rm $file"
         \rm $file
end

foreach file (*)
   grep 'This file was automatically generated by SWIG' $file > /dev/null || continue
   echo "\rm $file"
         \rm $file
end

##-------------------------------- end --------------------------------------##
##-------------------------------- end --------------------------------------##
##-------------------------------- end --------------------------------------##

